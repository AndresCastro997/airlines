@page
@model EditarModel

<h1>Editar </h1>

<form method="POST" enctype="multipart/form-data"  >
  
  <div class="form-group">
    <label for="Nombre">Nombre</label>
    <input type="text" name="Nombre" class="form-control"
         id="Nombre" aria-describedby="" value="@Model.Detalle.Nombre">
        @* <input asp-for="NuevoEstudiante.Nombre" /> *@
    
  </div>
  <div class="form-group">
    <label for="PaisOrigen">Pais Origen</label>
    <input type="text" class="form-control"
        name="PaisOrigen" id="PaisOrigen" aria-describedby="" value="@Model.Detalle.PaisOrigen">
    
  </div>

   <div class="form-group">
    <label for="Calificacion">Calificacion</label>
    <input type="text" class="form-control"
        name="Calificacion" id="Calificacion" aria-describedby="" value="@Model.Detalle.Calificacion">
    
  </div>

  
  
  <a asp-page="./Index" class="btn btn-info">Regresar</a>
  <button type="submit" class="btn btn-primary">Actualizar</button>
</form>